@model FormSubmission.Models.UserForm

@{
    ViewData["Title"] = "Result";
}

<h2>Form Submitted Successfully!</h2>

<ul>
    <li><strong>Name:</strong> @Model.Name</li>
    <li><strong>Email:</strong> @Model.Email</li>
    <li><strong>Date of Birth:</strong> @Model.DateOfBirth.ToShortDateString()</li>
    <li><strong>Password:</strong> @Model.Password</li>
    <li><strong>Favorite Number:</strong> @Model.FavoriteNumber</li>
</ul>
